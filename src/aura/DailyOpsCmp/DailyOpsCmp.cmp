				<div onclick="{!c.openLocationProcedures}" class="slds-p-around_x-small bg-white slds-m-around_medium">
					<lightning:icon iconName="standard:service_crew" size="small" />
					&nbsp;&nbsp;LOCATION PROCEDURES
                </div>
				<div onclick="{!c.openScrubOrderForm}" class="slds-p-around_x-small bg-white slds-m-around_medium">
                	<lightning:icon iconName="custom:custom24" size="small" />
                	&nbsp;&nbsp;SCRUB ORDER FORM 
                </div>
                <div onclick="{!c.openGuestRelatedForm}" class="slds-p-around_x-small bg-white slds-m-around_medium">
