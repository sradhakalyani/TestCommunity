                    <button onclick="{!c.openBusinessSupport}" style="background-color: rgb(255, 14, 73);border-color: #e1004a;" class="slds-button slds-button_brand">BACK</button>
                </div>
            </div>
        </div>
		<div class="{! v.stage == '32'?'slds-show':'slds-hide'}">
