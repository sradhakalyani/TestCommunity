    {'label': 'Spray bottle broken', 'value': 'option1'},
    {'label': 'Oil bottle broken', 'value': 'option2'},
	{'label': 'Floor Scrapper Missing', 'value': 'option3'},
	{'label': 'Floor Scrapper Missing', 'value': 'option4'},
	{'label': 'Mirror broken', 'value': 'option5'},
