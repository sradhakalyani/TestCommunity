        <div class="{! v.stage == '1'?'slds-show':'slds-hide'}">
        	<div class="slds-p-around_x-small" style="font-size: 13px;background: rgb(255, 14, 73);color: white;">DAILY OPS</div>
        	<div>
                <div class="slds-p-around_x-small bg-white slds-m-around_medium">
					<lightning:icon iconName="custom:custom18" size="small"/>
