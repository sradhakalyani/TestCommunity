                </div>
                <div class="slds-p-around_x-small bg-white slds-m-around_medium">
                	<lightning:icon iconName="standard:marketing_actions" size="small" />
                	&nbsp;&nbsp;REGISTER PROCEDURES
                </div>
                <div style="cursor:pointer;border-radius:3px;text-align:center;margin: 2%;padding:1%;">
                    <button onclick="{!c.openOnlineForms}" style="background-color: rgb(255, 14, 73);border-color: #e1004a;" class="slds-button slds-button_brand">BACK</button>
                </div>
            </div>
        </div>
        <div class="{! v.stage == '4'?'slds-show':'slds-hide'}">
            <div style="padding: 1%;background: rgb(255,14,73);color: white;"><span class="bc-nav" onclick="{!c.openDailyOps}">DAILY OPS</span> > <span class="bc-nav" onclick="{!c.openBusinessSupport}">BUSINESS SUPPORT</span> > <span class="bc-nav" onclick="{!c.openCreateSupportTickets}">CREATE SUPPORT TICKETS</span> > SOFTWARE AND HARDWARE ISSUES</div>
            <div style="cursor:pointer;border-radius:3px;background:white;margin: 6%;padding:1%;">   
				<lightning:card iconName="standard:service_crew" title="SOFTWARE AND HARDWARE ISSUES" >
					<div class="slds-p-top--small slds-p-horizontal_large slds-p-bottom--small">
                    	<p style="color:rgb(255, 14, 73);">Click one of the options listed below </p>
						<lightning:tree class="issuetree" items="{!v.items1}" onselect="{!c.listSelect}"/>        
                	</div>
        		</lightning:card>    
            </div>
            <div style="cursor:pointer;border-radius:3px;text-align:center;margin: 2%;padding:1%;">
                <button onclick="{!c.openCreateSupportTickets}" style="background-color: rgb(255, 14, 73);border-color: #e1004a;" class="slds-button slds-button_brand">BACK</button>
            </div>
        </div>
		<div class="{! v.stage == '42'?'slds-show':'slds-hide'}">
            <div style="padding: 1%;background: rgb(255,14,73);color: white;"><span class="bc-nav" onclick="{!c.openDailyOps}">DAILY OPS</span> > <span class="bc-nav" onclick="{!c.openOnlineForms}">ONLINE FORMS</span> > <span class="bc-nav" onclick="{!c.openDailyOperations}">DAILY OPS</span> >LOCATION PROCEDURES</div>
            <div style="cursor:pointer;border-radius:3px;background:white;margin: 6%;padding:1%;">   
				<lightning:card iconName="standard:service_crew" title="LOCATION PROCEDURES" >
					<div class="slds-p-top--small slds-p-horizontal_large slds-p-bottom--small">
                    	<p style="color:rgb(255, 14, 73);">Click one of the options listed below </p>
						<lightning:tree class="issuetree" items="{!v.items2}" onselect="{!c.listSelect}"/>        
                	</div>
        		</lightning:card>    
            </div>
            <div style="cursor:pointer;border-radius:3px;text-align:center;margin: 2%;padding:1%;">
                <button onclick="{!c.openDailyOperations}" style="background-color: rgb(255, 14, 73);border-color: #e1004a;" class="slds-button slds-button_brand">BACK</button>
            </div>
        </div>
		<div class="{! v.stage == '43'?'slds-show':'slds-hide'}">
            <div style="padding: 1%;background: rgb(255,14,73);color: white;"><span class="bc-nav" onclick="{!c.openDailyOps}">DAILY OPS</span> > <span class="bc-nav" onclick="{!c.openOnlineForms}">ONLINE FORMS</span> > <span class="bc-nav" onclick="{!c.openDailyOperations}">DAILY OPS</span> >SCRUB ORDER FORM</div>
            <div style="cursor:pointer;border-radius:3px;background:white;margin: 6%;padding:1%;">   
				<lightning:card iconName="standard:service_crew" title="SCRUB ORDER FORM" >
					<div class="slds-p-top--small slds-p-horizontal_large slds-p-bottom--small">
                    	<p style="color:rgb(255, 14, 73);">Click one of the options listed below </p>
						<lightning:tree class="issuetree" items="{!v.items3}" onselect="{!c.listSelect}"/>        
                	</div>
        		</lightning:card>    
            </div>
            <div style="cursor:pointer;border-radius:3px;text-align:center;margin: 2%;padding:1%;">
                <button onclick="{!c.openDailyOperations}" style="background-color: rgb(255, 14, 73);border-color: #e1004a;" class="slds-button slds-button_brand">BACK</button>
            </div>
        </div>
		<div class="{! v.stage == '44'?'slds-show':'slds-hide'}">
            <div style="padding: 1%;background: rgb(255,14,73);color: white;"><span class="bc-nav" onclick="{!c.openDailyOps}">DAILY OPS</span> > <span class="bc-nav" onclick="{!c.openOnlineForms}">ONLINE FORMS</span> > <span class="bc-nav" onclick="{!c.openDailyOperations}">DAILY OPS</span> >GUEST RELATED</div>
            <div style="cursor:pointer;border-radius:3px;background:white;margin: 6%;padding:1%;">   
				<lightning:card iconName="standard:service_crew" title="GUEST RELATED" >
					<div class="slds-p-top--small slds-p-horizontal_large slds-p-bottom--small">
                    	<p style="color:rgb(255, 14, 73);">Click one of the options listed below </p>
						<lightning:tree class="issuetree" items="{!v.items4}" onselect="{!c.listSelect}"/>        
                	</div>
        		</lightning:card>    
            </div>
            <div style="cursor:pointer;border-radius:3px;text-align:center;margin: 2%;padding:1%;">
                <button onclick="{!c.openDailyOperations}" style="background-color: rgb(255, 14, 73);border-color: #e1004a;" class="slds-button slds-button_brand">BACK</button>
            </div>
        </div>
    </div>
    <div class="{!v.popupcnt == '1' ? 'slds-show' : 'slds-hide'}" style="height: 640px;">
        <section role="dialog" tabindex="-1" aria-labelledby="modal-heading-01" aria-modal="true" aria-describedby="modal-content-id-1" class="slds-modal slds-fade-in-open">
            <div class="slds-modal__container ipad-height">
                <header class="slds-modal__header" style="background: rgb(255, 14, 73);color: #FFFFFF;padding: 1rem">
                    <button style="top: -2rem;font-size: 20px;" class="slds-button slds-button_icon slds-modal__close slds-button_icon-inverse" title="Close" onclick="{!c.cancel}">X</button>
                    <h2 id="modal-heading-01" class="slds-text-heading_medium">COMPUTERS SERVERS AND WORKSTATIONS ISSUE FORM</h2>
                </header>
			<div class="slds-modal__content slds-p-around_medium">
    			<lightning:layoutItem >
					<lightning:input class="formitem" type="text" name="Franchisee Name" label="Franchisee Name" value="EWC Franchisee Name"/> 
  				</lightning:layoutItem>
    			<lightning:layoutItem >
                    <lightning:formattedDateTime class="formitem" value="{!v.today}" year="numeric" month="numeric" day="numeric"  hour="2-digit" minute="2-digit" timeZoneName="short" />
    			</lightning:layoutItem>
    			<lightning:layoutItem >
        			<lightning:select class="formitem" aura:id="fieldID1" name="select1" label="Best Way to Reach you" required="true">
        				<option value="">select one option</option>
        				<option value="1">Home Phone</option>
        				<option value="2">Mobile Phone</option>
        				<option value="3">Work Phone</option>
        				<option value="4">Email</option>
    				</lightning:select>
    			</lightning:layoutItem>
    			<lightning:layoutItem >
    				<lightning:select class="formitem" aura:id="fieldID1" name="select2" label="Please Select Your Location Center" required="true">
        				<option value="">select the center</option>
        				<option value="1">Center1</option>
        				<option value="2">Center2</option>
        				<option value="3">Center3</option>
        				<option value="4">Center4</option>
    				</lightning:select>
				</lightning:layoutItem>
    			<lightning:layoutItem >
					<lightning:select class="formitem" name="select3" aura:id="fieldID1" label="What computer are you experiencing trouble with?" required="true">
        				<option value="">select the computer</option>
        				<option value="1">C1</option>
        				<option value="2">C2</option>
        				<option value="3">C3</option>
        				<option value="4">C4</option>
    				</lightning:select>
    			</lightning:layoutItem>
    			<lightning:layoutItem >
    				<lightning:textarea class="formitem" name="myTextArea" value="Enter the details of the issue" label="Please give a detailed description of the issue you are receiving" />
    			</lightning:layoutItem>
           
            </div>
                <footer class="slds-modal__footer">
                    <lightning:button class="formbutton" variant="neutral" name="{!v.popupcnt}" label="SUBMIT" onclick="{!c.save}"/>
                    <lightning:button class="formbutton" variant="neutral" label="CANCEL" onclick="{!c.cancel}"/>
                </footer>
            </div>
        </section>
        <div class="slds-backdrop slds-backdrop_open" style="background-size:cover;background-image: url('http://www.waxcenter.com/images/background-reservation.jpg')"></div>
    </div>
    <div class="{!v.popupcnt == '2' ? 'slds-show' : 'slds-hide'}" style="height: 640px;">
		<section role="dialog" tabindex="-1" aria-labelledby="modal-heading-01" aria-modal="true" aria-describedby="modal-content-id-1" class="slds-modal slds-fade-in-open">
            <div class="slds-modal__container ipad-height">
                <header class="slds-modal__header" style="background: rgb(255, 14, 73);color: #FFFFFF;padding: 1rem">
                    <button style="top: -2rem;font-size: 20px;" class="slds-button slds-button_icon slds-modal__close slds-button_icon-inverse" title="Close" onclick="{!c.cancel}">X</button>
                    <h2 id="modal-heading-01" class="slds-text-heading_medium">INSTALLATION AND UPDATE SOFTWARE REQUEST FORM</h2>
                </header>
			<div class="slds-modal__content slds-p-around_medium">
    			<lightning:layoutItem >
					<lightning:input class="formitem" type="text" name="Franchisee Name" label="Franchisee Name" value="EWC Franchisee Name"/> 
  				</lightning:layoutItem>
    			<lightning:layoutItem >
                    <lightning:formattedDateTime class="formitem" value="{!v.today}" year="numeric" month="numeric" day="numeric"  hour="2-digit" minute="2-digit" timeZoneName="short" />
    			</lightning:layoutItem>
    			<lightning:layoutItem >
        			<lightning:select class="formitem" aura:id="fieldID2" name="select1" label="Best Way to Reach you" required="true">
        				<option value="">select one option</option>
        				<option value="1">Home Phone</option>
        				<option value="2">Mobile Phone</option>
        				<option value="3">Work Phone</option>
        				<option value="4">Email</option>
    				</lightning:select>
    			</lightning:layoutItem>
    			<lightning:layoutItem >
    				<lightning:select class="formitem" aura:id="fieldID2" name="select2" label="Please Select Your Location Center" required="true">
        				<option value="">select the center</option>
        				<option value="1">Center1</option>
        				<option value="2">Center2</option>
        				<option value="3">Center3</option>
        				<option value="4">Center4</option>
    				</lightning:select>
				</lightning:layoutItem>
    			<lightning:layoutItem >
					<lightning:select class="formitem" name="select3" aura:id="fieldID2" label="What computer are you experiencing trouble with?" required="true">
        				<option value="">select the computer</option>
        				<option value="1">C1</option>
        				<option value="2">C2</option>
        				<option value="3">C3</option>
        				<option value="4">C4</option>
    				</lightning:select>
    			</lightning:layoutItem>
    			<lightning:layoutItem >
    				<lightning:textarea class="formitem" name="myTextArea" aura:id="fieldID" value="Enter the details of the issue" label="Please list the website(s) that you need access to in order to install or update your software." required="true" />
    			</lightning:layoutItem>
				<lightning:layoutItem >
    				<lightning:textarea class="formitem" name="myTextArea" value="Comments" label="Comments" />
    			</lightning:layoutItem>           
            </div>
                <footer class="slds-modal__footer">
                    <lightning:button class="formbutton" variant="neutral" name="{!v.popupcnt}" label="SUBMIT" onclick="{!c.save}"/>
                    <lightning:button class="formbutton" variant="neutral" label="CANCEL" onclick="{!c.cancel}"/>
                </footer>
            </div>
        </section>
        <div class="slds-backdrop slds-backdrop_open" style="background-size:cover;background-image: url('http://www.waxcenter.com/images/background-reservation.jpg')"></div>
  	</div>
